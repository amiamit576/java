package Array;
import java.util.Scanner;
public class MethodToCreateArraysize {

	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("The element in Array are:");
		
	
		int[]a=arrayOfSize(10);
		
		for(int i:a) {
			 i =sc.nextInt();
			System.out.print(i+" ");
		}
		
		

	}
	public static int[] arrayOfSize(int size) {
		int[]arr=new int[size];
		return arr;
		
		
		
	}
	
	

}
